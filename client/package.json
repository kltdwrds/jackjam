    "dotenv": "^8.2.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
