{
  "name": "jackjam-client",
  "description": "Jam from your bedroom",
  "version": "0.0.1",
  "author": { 
    "name" : "Kyle T. Edwards",
    "email" : "edwards.ky1@gmail.com",
    "url" : "https://github.com/kltdwrds"
  },
  "build": {
    "appId": "com.jackjam",
    "extraResources": [
      "resources/**/*"
    ],
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "scripts": {
