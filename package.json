{
  "name": "jackjam-client",
  "description": "Jam from your bedroom",
  "version": "0.0.1",
  "author": {
    "name": "Kyle T. Edwards",
    "email": "edwards.ky1@gmail.com",
    "url": "https://github.com/kltdwrds"
  },
  "browserslist": "Electron 9.2",
  "main": "dist/main.js",
  "build": {
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "",
        "filter": [
          "**/*"
        ]
      }
    ],
    "appId": "com.jackjam",
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "scripts": {
    "dev": "nodemon --watch webpack.config.js --exec \"webpack serve --mode development\"",
    "electron": "electron ./dist/main.js",
    "build": "webpack --mode production --progress",
    "version": "grep 'version' package.json | cut -d '\"' -f4",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint --ext .tsx --ext .ts ./src",
    "lint:eslint:fix": "eslint --ext .tsx --ext .ts --fix ./src",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:json": "prettier -c '**/*.json'",
    "lint:json:fix": "prettier --write '**/*.json'",
    "lint": "run-s -c lint:ts lint:eslint lint:css lint:json",
    "lint:fix": "run-s -c lint:ts lint:eslint:fix lint:css lint:json:fix"
  },
  "repository": "https://github.com/kltdwrds/jackjam",
  "keywords": [],
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.8",
    "@chakra-ui/system": "^1.0.0-rc.8",
    "@chakra-ui/theme": "^1.0.0-rc.8",
    "framer-motion": "^2.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-numeric-separator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^3.0.0",
    "electron": "^9.3.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "html-webpack-plugin": "^4.5.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "source-map-loader": "^0.2.4",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
