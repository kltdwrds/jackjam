{
  "name": "jackjam-client",
  "description": "Jam from your bedroom",
  "version": "0.0.1",
  "author": {
    "name": "Kyle T. Edwards",
    "email": "edwards.ky1@gmail.com",
    "url": "https://github.com/kltdwrds"
  },
  "browserslist": "Electron 9.2",
  "main": "dist/main.js",
  "build": {
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "resources",
        "to": "",
        "filter": [
          "**/*"
        ]
      }
    ],
    "appId": "com.jackjam",
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "scripts": {
    "dev": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --mode development\"",
    "electron": "electron ./dist/main.js",
    "build": "webpack --mode production --progress --display=minimal --display-entrypoints=true",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint --ext .tsx --ext .ts ./src",
    "lint:eslint:fix": "eslint --ext .tsx --ext .ts --fix ./src",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:json": "prettier -c '**/*.json'",
    "lint:json:fix": "prettier --write '**/*.json'",
    "lint": "run-s -c lint:ts lint:eslint lint:css lint:json",
    "lint:fix": "run-s -c lint:ts lint:eslint:fix lint:css lint:json:fix"
  },
  "repository": "https://github.com/kltdwrds/jackjam",
  "keywords": [],
  "dependencies": {
    "@chakra-ui/core": "^1.0.0-rc.2",
    "@chakra-ui/system": "^1.0.0-rc.2",
    "@chakra-ui/theme": "^1.0.0-rc.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.1"
  }
}
