{
  "files.associations": {
    "*.html.jinja2": "jinja-html",
    "*.txt.jinja2": "jinja-raw",
    ".stylelintrc": "json5",
    ".babelrc": "json5"
  },
  "eslint.lintTask.enable": true,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.rulers": [80]
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.rulers": [80]
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.rulers": [80]
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": null,
    "editor.formatOnSave": false,
    "editor.tabSize": 2,
    "editor.rulers": [80]
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true
  },
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.format.enable": false
}
